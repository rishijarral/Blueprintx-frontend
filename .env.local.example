# BlueprintX Frontend - Environment Configuration
# =============================================================================
# Copy this file to .env.local and fill in your values
# =============================================================================

# =============================================================================
# SUPABASE AUTH
# =============================================================================
# Your Supabase project URL (from Project Settings > API)
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co

# Supabase publishable/anon key (safe to expose in browser)
# New format: sb_publishable_xxxxx or legacy JWT format (eyJ...)
NEXT_PUBLIC_SUPABASE_ANON_KEY=sb_publishable_your_key_here

# =============================================================================
# API CONFIGURATION
# =============================================================================
# Backend API URL (Rust server)
NEXT_PUBLIC_API_URL=http://localhost:8080
